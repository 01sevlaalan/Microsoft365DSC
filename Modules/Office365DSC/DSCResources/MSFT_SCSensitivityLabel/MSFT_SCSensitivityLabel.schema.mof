[ClassVersion("1.0.0")]
Class MSFT_SCLabelToolTips
{
    [Write, Description("Tooltip language")] String Locale;
    [Write, Description("Tooltip display name.")] String ToolTip ;
};
Class MSFT_SCLabelDisplayNames
{
    [Write, Description("Tooltip language")] String Locale;
    [Write, Description("Tooltip display name.")] String DisplayName ;
};
[ClassVersion("1.0.0")]
Class MSFT_SCLabelLocaleSettings
{
    [Write, Description("The LocaleSettings tooltips"),EmbeddedInstance("MSFT_SCLabelToolTips")] String[] Toolips;
    [Write, Description("The LocaleSettings displayname"),EmbeddedInstance("MSFT_SCLabelDisplayNames")] String[] DisplayNames;
